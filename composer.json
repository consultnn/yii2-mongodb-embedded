{
    "name": "consultnn/yii2-mongodb-embedded",
    "description": "Yii2 behaviors implement handling of mongodb embedded documents",
    "license": "MIT",
    "authors": [
        {
            "name": "ICC",
            "email": "tech@consultnn.ru"
        },
        {
            "name": "Sergey Sipatov",
            "role": "developer",
            "email": "sergey.sipatov@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": "2.0.*"
    },
    "require-dev" : {
        "yiisoft/yii2-codeception": "*",
        "codeception/codeception": "*"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer" : true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": {
            "consultnn\\embedded\\": "."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
